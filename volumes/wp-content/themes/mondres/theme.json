							}
						},
						"color": {
							"text": "var(--wp--preset--color--accent-4)"
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"spacing": {
					"margin": {
						"bottom": "0px",
						"top": "5px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-content": {
